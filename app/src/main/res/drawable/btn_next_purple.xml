<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <shape><solid android:color="#FF6200EE"/><stroke android:width="3dp" android:color="#FFFFFF"/><corners android:radius="12dp"/></shape>
    </item>
    <item android:state_pressed="true">
        <shape><solid android:color="#E91E63"/><corners android:radius="12dp"/></shape>
    </item>
    <item><shape><solid android:color="#FF4081"/><stroke android:width="2dp" android:color="#FFFFFF"/><corners android:radius="12dp"/></shape></item>
</selector>
